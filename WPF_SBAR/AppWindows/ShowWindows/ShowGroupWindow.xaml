<Window x:Class="WPF_SBAR.AppWindows.ShowWindows.ShowGroupWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_SBAR.AppWindows.ShowWindows"
        mc:Ignorable="d"
        Title="Выбор группы" Height="600" Width="700" Loaded="Window_Loaded" 
		WindowStartupLocation="CenterScreen">
	<DockPanel Background="#FFC2C3AB" KeyDown="DockPanel_KeyDown">
		<WrapPanel DockPanel.Dock="Bottom"  HorizontalAlignment="Center"  >
			<Button Name="OK" Click="OK_Click" Margin="0,0,30,5" Width="85">Выбрать</Button>
			<Button Name="Cancel" Click="Cancel_Click"  Margin="30,0,0,5" Width="85">Отменить</Button>
		</WrapPanel>
		<ScrollViewer Name="Contaner"  DockPanel.Dock="Top">

		</ScrollViewer>
		
	</DockPanel>
    
</Window>
